<?xml version="1.0" encoding="UTF-8" ?>
<ContentPage
    x:Class="Xalendar.Sample.Views.TaskManagementTheme"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:xal="http://xalendar.com/schemas/xaml"
    Padding="0,20,0,0">
    <ContentPage.Resources>
        <ResourceDictionary>
            <xal:TaskManagement x:Key="TaskManagementTheme" />
        </ResourceDictionary>
    </ContentPage.Resources>

    <ContentPage.Content>
        <xal:CalendarView Theme="{StaticResource TaskManagementTheme}" />
    </ContentPage.Content>
</ContentPage>
